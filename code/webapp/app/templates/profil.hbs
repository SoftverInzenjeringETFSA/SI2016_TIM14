<div class="container-fluid">
  <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css">
    <div class="row content">
        {{side-navigation}}
        <div class="col-sm-9">
            <div class="container">
                <div class="col-md-5  toppad  pull-right col-md-offset-3 ">
                </div>
                <div class="row">
                    <div class="col-sm-3">
                        <div class="image-all">
                            <div class="row">
                                <img class="img-responsive" src="http://queenaration.com/wp-content/uploads/2016/07/QueenAvatar-128x128.jpg" alt="Quenn">
                            </div>
                            <div class="row">
                                <div class="rating">
                                    <div class="stars">
                                        <form action="">
                                            <input class="star star-5" id="star-5" type="radio" name="star" />
                                            <label class="star star-5" for="star-5"></label>
                                            <input class="star star-4" id="star-4" type="radio" name="star" />
                                            <label class="star star-4" for="star-4"></label>
                                            <input class="star star-3" id="star-3" type="radio" name="star" />
                                            <label class="star star-3" for="star-3"></label>
                                            <input class="star star-2" id="star-2" type="radio" name="star" />
                                            <label class="star star-2" for="star-2"></label>
                                            <input class="star star-1" id="star-1" type="radio" name="star" />
                                            <label class="star star-1" for="star-1"></label>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-9">
                        <div class="profileTopForm">
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-sm-2">
                                        <label for="datumRodjenja">NAME: </label>
                                    </div>
                                    <div class="col-sm-10">
                                        <textarea readonly class="form-control" rows="1" id="comment">{{ session.data.authenticated.korisnik.firstName}}</textarea>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-2">
                                        <label for="Spol">SURNAME:</label>
                                    </div>
                                    <div class="col-sm-10">
                                        <textarea readonly class="form-control" rows="1" id="comment">{{ session.data.authenticated.korisnik.lastName }}</textarea>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-2">
                                        <label for="zanimanje">WORK:</label>
                                    </div>
                                    <div class="col-sm-10">
                                        <textarea readonly class="form-control" rows="1" id="comment">{{ session.data.authenticated.korisnik.zanimanje }}</textarea>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-2">
                                        <label for="grad">CITY:</label>
                                    </div>
                                    <div class="col-sm-10">
                                        <textarea readonly class="form-control" rows="1" id="comment">{{ session.data.authenticated.korisnik.location }}</textarea>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-2">
                                        <label for="jezici">EMAIL:</label>
                                    </div>
                                    <div class="col-sm-10">
                                        <textarea readonly class="form-control" rows="1" id="comment">{{ session.data.authenticated.korisnik.email }}</textarea>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="ovodole">
                        <div class="about-me-form">
                            <label for="about-me"><SMALL> ABOUT ME </SMALL></label>
                            <textarea readonly class="form-control" rows="5" id="about-me">
                            {{ session.data.authenticated.korisnik.omeni }}</textarea>
                        </div>
                        <br>
                        <div class="interests-form">
                            <label for="interests"><SMALL> I'M INTERESTED IN... </SMALL></label>
                            <textarea readonly class="form-control" rows="5" id="interests">{{ session.data.authenticated.korisnik.interesovanja }}</textarea>
                        </div>
                    </div>
                    {{#link-to 'edit-profil'}} <button class="dugmic"> Edit Profile </button> {{/link-to}}
                </div>
                {{#link-to 'edit-sifra'}} <button class="dugmic"> Change Password </button> {{/link-to}}
            </div>
        </div>
    </div>
</div>
